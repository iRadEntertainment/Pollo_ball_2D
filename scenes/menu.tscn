[sub_resource type="GDScript" id=37]
script/source = "extends PanelContainer

func _ready():
	ntw.connect(\"announcements_updated\",self,\"update_news\")
	$vbox/exp.hide()

func update_news(val):
	ann_id = 0
	show_announcement(ann_id)
	populate_expanded_box()

func show_announcement(id):
	if not ntw.announcements.empty():
		$vbox/bg/hbox/user.text = ntw.announcements[id][0]
		$vbox/bg/hbox/text.text = ntw.announcements[id][1]
		# start the _process(d) for announcements loop if there is more than one announcement
		set_process(ntw.announcements.size() > 1)

func populate_expanded_box():
	for ann_instance in $vbox/exp.get_children():
		if ann_instance.name != \"add_news\":
			ann_instance.queue_free()
	
	if not ntw.announcements.empty():
		for id_ann in range(ntw.announcements.size()):
			var row = HBoxContainer.new()
			var ann_user = Label.new()
			ann_user.rect_min_size.x = $vbox/bg/hbox/user.rect_min_size.x
			ann_user.text = ntw.announcements[id_ann][0]
			var sep1 = VSeparator.new()
			var ann_mess = Label.new()
			ann_mess.text = ntw.announcements[id_ann][1]
			ann_mess.size_flags_horizontal = SIZE_EXPAND_FILL
			
			var sep2 = VSeparator.new()
			var delete_button = Button.new()
			delete_button.text = \"delete\"
			delete_button.connect(\"pressed\",self,\"delete_announcement\",[id_ann])
			
			for node in [ann_user, sep1, ann_mess, sep2, delete_button]:
				row.add_child(node)
			
			$vbox/exp.add_child(row)
	
#	$vbox/exp.move_child($vbox/exp/add_news, ntw.announcements.size())

func delete_announcement(id_ann):
	ntw.rpc_id(1,\"remove_announcement\",id_ann)


var ann_id = 0
var time_elapsed = 0
var scroll_time = 4

func _process(d):
	time_elapsed += d
	if time_elapsed > scroll_time:
		time_elapsed = 0
		ann_id += 1
		if ann_id >= ntw.announcements.size():
			ann_id = 0
		show_announcement(ann_id)
"

