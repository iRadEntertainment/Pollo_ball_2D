[sub_resource type="GDScript" id=51]
script/source = "extends TextEdit

func _ready():
	ntw.connect(\"server_log_updated\",self,\"_compile_server_log\")
	ntw.connect(\"server_log_last_entry_updated\",self,\"_add_line\")

func _compile_server_log():
	text = \"\"
	for line in ntw.server_log:
		text += parse_log_message(line)

func _add_line(line):
	text += parse_log_message(line)

func parse_log_message(line):
	var dic = OS.get_datetime_from_unix_time(line[0])
	var time = [dic[\"day\"],dic[\"month\"],dic[\"hour\"],dic[\"minute\"],dic[\"second\"]]
	var time_str   = \"%02d/%02d %02d:%02d:%02d\"%time
	
	var format_str = str(time_str,\" \",line[1],\": \",line[2],\"\\n\")
	return format_str
"

